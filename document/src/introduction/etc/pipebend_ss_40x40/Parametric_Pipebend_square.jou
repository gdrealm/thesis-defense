reset
#{n = 40}


# Create points
create vertex  {0}  {-n/2} 0
create vertex  {n}  {-n/2} 0
create vertex  {n}  {n/2} 0
create vertex  {0}  {n/2} 0

create vertex  0    {n/2-n/10} 0
create vertex  0    {n/2-3*n/10} 0
create vertex  0 {n/2-n/10} 0
create vertex  0 {n/2-3*n/10} 0

create vertex  {n-n/10} {-n/2} 0
create vertex  {n-3*n/10} {-n/2} 0

create curve vertex 1  vertex 10  
create curve vertex 9  vertex 10  
create curve vertex 9  vertex 2  
create curve vertex 3 vertex 2  
create curve vertex 3 vertex 4 
create curve vertex 7  vertex 4 
create curve vertex 7  vertex 6  
create curve vertex 1 vertex 6 


create surface curve 1 2 3 4 5 6 7 8

curve 4 5  interval {n}
curve 4 5  scheme equal

curve 6 3  interval  {n/10}
curve   scheme equal

curve 8 1  interval {7*n/10}
curve 8 1  scheme equal

curve 7 2  interval {2*n/10}
curve 7 2  scheme equal

surface 1 scheme Auto
mesh surface 1 

block 1 surface 1
block 1 element type QUAD4

# inlet
Sideset 1 curve 7  
Nodeset 1 curve 7  

# stick
Sideset 2 curve 1 3 4 5 6 8
Nodeset 2 curve 1 3 4 5 6 8

# outlet
Sideset 3 curve 2  
Nodeset 3 curve 2  

surface 1 scale {5/n}


delete Vertex 8
delete Vertex 5
